,"import { OrbitControls } from 'three/addons/controls/OrbitControls.js';",[],{"link_type":40},{"primary":430,"items":432,"id":442,"slice_type":44,"slice_label":7},{"identifier":431,"section_title":7},"renderer",[433],{"is_anchor":31,"class":431,"title":434,"rich":435,"video":439,"path":440,"code_lang":41,"code":441},"Renderer",[436],{"type":36,"text":437,"spans":438},"Creating a renderer and applying it to the DOM",[],{"link_type":40},"main.js","const canvas = document.querySelector('canvas#sketch');\nlet pixelRatio = Math.min(window.devicePixelRatio, 2)\nconst renderer = new THREE.WebGLRenderer({\n\tcanvas: canvas,\n\talias: true,\n\talpha: true\n});+\nrenderer.setClearColor(0x000000, 0)\nrenderer.setSize( window.innerWidth, window.innerHeight );\nrenderer.setPixelRatio(pixelRatio)","doc_item$69234942-da8d-49c8-a5b4-44221fd04685",{"primary":444,"items":446,"id":455,"slice_type":44,"slice_label":7},{"identifier":445,"section_title":7},"scene-camera",[447],{"is_anchor":129,"class":445,"title":448,"rich":449,"video":453,"path":440,"code_lang":41,"code":454},"Scene & Camera",[450],{"type":36,"text":451,"spans":452},"Creating a scene to receive our 3D elements and utilities and set up of the Camera",[],{"link_type":40},"const scene = new THREE.Scene();\nconst camera = new THREE.PerspectiveCamera( 35, window.innerWidth / window.innerHeight, 0.1, 100 );\ncamera.position.z = 6;","doc_item$b42a2f3c-b311-489d-9691-679a2bcb2617",{"primary":457,"items":459,"id":468,"slice_type":44,"slice_label":7},{"identifier":458,"section_title":7},"mesh",[460],{"is_anchor":31,"class":458,"title":461,"rich":462,"video":466,"path":440,"code_lang":41,"code":467},"Mesh",[463],{"type":36,"text":464,"spans":465},"Creating a mesh (geometry + material) and add it to the scene",[],{"link_type":40},"const geometry = new THREE.BoxGeometry( 1, 1, 1 );\nconst material = new THREE.MeshBasicMaterial( { color: 0xff0000 } );\nconst cube = new THREE.Mesh( geometry, material );\nscene.add( cube );","doc_item$f446aaee-52bd-489d-97ba-39cab23fcb3a",{"primary":470,"items":472,"id":481,"slice_type":44,"slice_label":7},{"identifier":471,"section_title":7},"orbitcontrol",[473],{"is_anchor":129,"class":7,"title":474,"rich":475,"video":479,"path":440,"code_lang":41,"code":480},"OrbitControl",[476],{"type":36,"text":477,"spans":478},"Utility to manipulate the camera and inspect the scene",[],{"link_type":40},"const controls = new OrbitControls( camera, canvas );","doc_item$5f77ea6b-2374-4786-9062-1fcfc8fbf6be",{"primary":483,"items":485,"id":494,"slice_type":44,"slice_label":7},{"identifier":484,"section_title":7},"loop",[486],{"is_anchor":31,"class":484,"title":487,"rich":488,"video":492,"path":7,"code_lang":41,"code":493},"Loop",[489],{"type":36,"text":490,"spans":491},"Looped function to update the render application 60 times por second.",[],{"link_type":40},"function animate() {\n\trequestAnimationFrame( animate );\n\tcube.rotation.x += 0.005;\n\tcube.rotation.y += 0.01;\n\trenderer.render( scene, camera );\n}","doc_item$1c174b3d-c886-4e3d-ae3c-f2b25928e444",{"primary":496,"items":498,"id":507,"slice_type":44,"slice_label":7},{"identifier":497,"section_title":7},"resize",[499],{"is_anchor":129,"class":497,"title":500,"rich":501,"video":505,"path":7,"code_lang":41,"code":506},"On Resize",[502],{"type":36,"text":503,"spans":504},"Function that update our 3D enviroment parameters on Window Resize",[],{"link_type":40},"function onResize() {\n    camera.aspect = window.innerWidth / window.innerHeight;\n    camera.updateProjectionMatrix();\n\n\tpixelRatio = Math.min(window.devicePixelRatio, 2)\n\trenderer.setSize( window.innerWidth, window.innerHeight );\n\trenderer.setPixelRatio(pixelRatio)\n}\nwindow.addEventListener( 'resize', onResize, false );","doc_item$ff077c76-9547-4ed8-92a1-949f1a5297a7",{"id":509,"uid":510,"url":7,"type":8,"href":511,"tags":512,"first_publication_date":513,"last_publication_date":514,"slugs":515,"linked_documents":518,"lang":21,"alternate_languages":519,"data":520},"ZSw1-RAAACUAyVE3","06-loading-assets","https://slides-testing.cdn.prismic.io/api/v2/documents/search?ref=ZS66BRIAACgA2Nfl&q=%5B%5B%3Ad+%3D+at%28document.id%2C+%22ZSw1-RAAACUAyVE3%22%29+%5D%5D",[11],"2023-10-15T19:19:44+0000","2023-10-17T16:44:53+0000",[516,517,69],"06.-loading-assets","06---loading-assets",[],[],{"page_title":521,"meta_title":521,"meta_name":7,"meta_description":7,"meta_type":7,"meta_url":7,"meta_img":522,"body":523},"06. Loading Assets",{},[524,543,557,571,588,602],{"primary":525,"items":527,"id":43,"slice_type":44,"slice_label":7},{"identifier":526,"section_title":521},"import-loaders",[528],{"is_anchor":31,"class":529,"title":530,"rich":531,"video":541,"path":7,"code_lang":41,"code":542},"import-loaders-section","Importing  Loaders",[532,535,538],{"type":36,"text":533,"spans":534},"We have to import THREE.js loaders to import assets as 3D Models, Textures, Fonts and etc, inside of our 3D environment. In this case we will use two Loaders:\n",[],{"type":242,"text":536,"spans":537},"GLTFLoader",[],{"type":242,"text":539,"spans":540},"DRACOLoader",[],{"link_type":40},"import { GLTFLoader } from \"three/examples/jsm/loaders/GLTFLoader.js\";\nimport { DRACOLoader } from \"three/examples/jsm/loaders/DRACOLoader.js\";",{"primary":544,"items":546,"id":556,"slice_type":44,"slice_label":7},{"identifier":545,"section_title":7},"instance-loaders",[547],{"is_anchor":31,"class":548,"title":549,"rich":550,"video":554,"path":7,"code_lang":41,"code":555},"instance-loaders-section","Instancing Loaders",[551],{"type":36,"text":552,"spans":553},"Instance loaders to use within our application, different type of assets requires a different loader.",[],{"link_type":40},"instanceLoaders() {\n    this.loaders = {};\n\n    this.loaders.dracoLoader = new DRACOLoader();\n    this.loaders.dracoLoader.setDecoderPath('https://www.gstatic.com/draco/versioned/decoders/1.5.5/');\n\n    this.loaders.gltfLoader = new GLTFLoader();\n    this.loaders.gltfLoader.setDRACOLoader(this.loaders.dracoLoader)\n\n    this.loaders.cubeTextureLoader = new THREE.CubeTextureLoader();\n}","doc_item$7e556005-3108-4ed8-8b69-95d8400fbbd6",{"primary":558,"items":560,"id":570,"slice_type":44,"slice_label":7},{"identifier":559,"section_title":7},"import-assets",[561],{"is_anchor":129,"class":562,"title":563,"rich":564,"video":568,"path":7,"code_lang":41,"code":569},"import-assets-section","Import Assets",[565],{"type":36,"text":566,"spans":567},"Instace the path of our assets",[],{"link_type":40},"// models\nimport Pumpkin from '../models/pumpkin.glb'\nimport Reaper from '../models/reaper.glb'\nimport Witch from '../models/witch.glb'","doc_item$df0622dd-8f9b-425c-8a6f-9dca54469f40",{"primary":572,"items":574,"id":587,"slice_type":44,"slice_label":7},{"identifier":573,"section_title":7},"assets-in-object",[575],{"is_anchor":129,"class":576,"title":577,"rich":578,"video":585,"path":7,"code_lang":41,"code":586},"assets-in-object-section","Assets into Object",[579,582],{"type":36,"text":580,"spans":581},"Alocating the assets in an Object with the purpose of creating a loader for all assets.",[],{"type":36,"text":583,"spans":584},"This Object will provide a name, url and also display the type for each asset.",[],{"link_type":40},"this.sources = [\n            {\n                name: 'pumpkin',\n                url: Pumpkin,\n                type: 'gltf'\n            },\n            {\n                name: 'reaper',\n                url: Reaper,\n                type: 'gltf'\n            },            \n            {\n                name: 'witch',\n                url: Witch,\n                type: 'gltf'\n            },\n            {\n                name: \"environmentMapTexture\",\n                type: \"cubeTexture\",\n                path: [\n                    \"https://raw.githubusercontent.com/Efetivos/gallery/master/avulsos/px.jpeg\",\n                    \"https://raw.githubusercontent.com/Efetivos/gallery/master/avulsos/nx.jpeg\",\n                    \"https://raw.githubusercontent.com/Efetivos/gallery/master/avulsos/py.jpeg\",\n                    \"https://raw.githubusercontent.com/Efetivos/gallery/master/avulsos/ny.jpeg\",\n                    \"https://raw.githubusercontent.com/Efetivos/gallery/master/avulsos/pz.jpeg\",\n                    \"https://raw.githubusercontent.com/Efetivos/gallery/master/avulsos/nz.jpeg\"\n                ]\n            }\n        ]","doc_item$8da122aa-c7c6-4963-817f-e78d9762fcc3",{"primary":589,"items":591,"id":601,"slice_type":44,"slice_label":7},{"identifier":590,"section_title":7},"loading-assets",[592],{"is_anchor":31,"class":593,"title":594,"rich":595,"video":599,"path":7,"code_lang":41,"code":600},"loading-assets-section","Loading assets",[596],{"type":36,"text":597,"spans":598},"Loading all assets and attribute them to right loader",[],{"link_type":40},"loadModels() {\n        this.objs = [];\n        this.items = [];\n        for (const source of this.sources) {\n            if (source.type === \"gltf\") {\n                this.loaders.gltfLoader.load(source.url, (file) => {\n                    this.objs.push({ name: source.name, model: file });\n                    this.sourceLoaded(source, file);\n                });\n            } else if (source.type === \"cubeTexture\") {\n                this.loaders.cubeTextureLoader.load(source.path, (file) => {\n                    this.sourceLoaded(source, file);\n                });\n            }\n        }\n    }","doc_item$e91438b3-e500-4767-a4c9-7a43c7fa5c97",{"primary":603,"items":605,"id":635,"slice_type":44,"slice_label":7},{"identifier":604,"section_title":7},"is-ready",[606,616,623,628],{"is_anchor":129,"class":607,"title":608,"rich":609,"video":615,"path":7,"code_lang":41,"code":7},"is-ready-section","Check loaded assets",[610,613],{"type":36,"text":611,"spans":612},"This function will verify and trigger an event once all assets have succesfully loaded.",[],{"type":36,"text":249,"spans":614},[],{"link_type":40},{"is_anchor":129,"class":7,"title":7,"rich":617,"video":621,"path":7,"code_lang":41,"code":622},[618],{"type":36,"text":619,"spans":620},"Let's instace a variable with the quantity of assets to be loaded, and also a counter to run inside of the funcion",[],{"link_type":40},"this.toLoad = this.sources.length;\nthis.loaded = 0;",{"is_anchor":129,"class":7,"title":624,"rich":625,"video":626,"path":7,"code_lang":41,"code":627},"Verifying function",[],{"link_type":40},"sourceLoaded(source, file) {\n        this.items[source.name] = file;\n\n        this.loaded++;\n\n        if (this.loaded === this.toLoad) {\n            this.isReady();\n        }\n    }",{"is_anchor":129,"class":7,"title":7,"rich":629,"video":633,"path":7,"code_lang":41,"code":634},[630],{"type":36,"text":631,"spans":632},"Run the function after all assets are loaded",[],{"link_type":40},"isReady() {\n    new Models(this.app, this.objs)\n    new Ambient(this.app, this.items.environmentMapTexture)    \n}","doc_item$b1319f49-45da-495c-8829-2da25a20de4b",{"id":637,"uid":638,"url":7,"type":8,"href":639,"tags":640,"first_publication_date":641,"last_publication_date":642,"slugs":643,"linked_documents":647,"lang":21,"alternate_languages":648,"data":649},"ZSbr1BAAACIAsW6f","01-overview","https://slides-testing.cdn.prismic.io/api/v2/documents/search?ref=ZS66BRIAACgA2Nfl&q=%5B%5B%3Ad+%3D+at%28document.id%2C+%22ZSbr1BAAACIAsW6f%22%29+%5D%5D",[11],"2023-10-11T18:39:22+0000","2023-10-16T18:52:23+0000",[644,140,645,646,400],"01.-course-overview","01---course-overview","01---the-syllabus",[],[],{"page_title":650,"meta_title":650,"meta_name":7,"meta_description":7,"meta_type":7,"meta_url":7,"meta_img":651,"body":652},"01. Course Overview",{},[653,656,677,693],{"primary":654,"items":655,"id":277,"slice_type":44,"slice_label":7},{"identifier":149,"section_title":650},[],{"primary":657,"items":658,"id":316,"slice_type":44,"slice_label":7},{"identifier":280,"section_title":7},[659],{"is_anchor":31,"class":283,"title